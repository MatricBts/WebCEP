create database funildevendas;
use funildevendas;

CREATE TABLE IF NOT EXISTS `usuarios` (
  `id` int(11) NOT NULL AUTO_INCREMENT,
 `nome_empresa` varchar(220) DEFAULT NULL,
  `cnpj` int(11) DEFAULT NULL,
  `telefone` int(12) DEFAULT NULL,
  `nome_responsavel` varchar(220) DEFAULT NULL,
  `email` varchar(220) DEFAULT NULL,
 `cep` int(11) DEFAULT NULL,
  `endereco` varchar(220) DEFAULT NULL,
  `numero` varchar(10) DEFAULT NULL,
  `complemento` varchar(120) DEFAULT NULL,
  `bairro` varchar(220) DEFAULT NULL,
  `cidade` varchar(220) DEFAULT NULL,
  `estado` varchar(2) DEFAULT NULL,
  `created` datetime NOT NULL,
  `modified` datetime DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB  DEFAULT CHARSET=latin1 AUTO_INCREMENT=2 ;

